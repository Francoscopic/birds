
{% extends 'pages/in/layout.html.twig' %}
{% use 'pages/layout/follows.html.twig' %}


{# HEADERS #}
    {% block darkmode_stylesheet %}
        {% if canvas.misc.theme_state == true %}
            <link rel="stylesheet" href="{{ asset('/build/dark.css') }}" watch="theme" />
        {% endif %}
    {% endblock %} 
{# HEADERS - END #}


{% block page_contents %}

    {% if canvas.notes.validation.check == true %}
        {% if canvas.notes.validation.my_profile == false %}
            <h1>You do not have access to view this page</h1>
        {% else %}
            {{ block('followers_page_one') }}
        {% endif %}
    {% else %}
        {# Account not found #}
        <h1>Account was not found</h1>
    {% endif %}
    
{% endblock page_contents %}



{% block script_modules %}
    <script type="module" type="text/javascript" src="{{  asset('/scripts/profile/profiles.js')}}" defer></script>
    {# <script type="module" type="text/javascript" src="{{  asset('/scripts/people/people.js')}}" defer></script> #}
{% endblock %}